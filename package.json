    "nestjs-pino": "^4.3.0",
    "npm-check-updates": "^17.1.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
